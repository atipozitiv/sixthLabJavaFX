<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<VBox alignment="BASELINE_CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.sixthlab.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox spacing="80" style="-fx-padding: 5 0 0 360;">
        <Button text="Круг" onAction="#circleClick"/>
        <Button text="Прямоугольник" onAction="#rectangleClick"/>
    </HBox>
    <BorderPane fx:id="root" minHeight="400" minWidth="1000" maxHeight="400" maxWidth="1000"/>
</VBox>